   this.coordinated.forEach(element => {
              // for showing signature on the screen
              if (element.Name == "Signature") {
                alert(creator.Userdata.UserSignature);
              }

              // for showing initial on the screen
              if (element.Name == "Initial") {
                alert(creator.Userdata.UserInitials);
              }
              // for showing date that user will sign the document
              if (element.Name == "DateSigned") {
                
                var today = new Date();
                var time =
                  today.getHours() +
                  ":" +
                  today.getMinutes() +
                  ":" +
                  today.getSeconds();
                alert("today" + time);
              }
              // for showing user name
              if (element.Name == "Name") {
                alert(creator.Userdata.UserName);
                alert("Name");
              }

              // for showing user email
              if (element.Name == "Email") {
                alert(creator.Userdata.UserEmail);
                alert("Email");
              }

              // for showing usercompnay
              if (element.Name == "Company") {
                alert(creator.Userdata.UserCompany);
              }
            });